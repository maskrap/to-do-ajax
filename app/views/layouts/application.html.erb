<!DOCTYPE html>
<html>
  <head>
    <div class="container">
      <title><%= yield(:title) %></title>
      <%= csrf_meta_tags %>
      <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
      <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    </div>
  </head>
  <div class="container">
    <%= yield(:navbar) %>
  </div>
  <body>
    <div class="container">
      <%= flash[:alert] %>
      <%= flash[:notice] %>
      <%= yield %>
    </div>
  </body>
</html>
